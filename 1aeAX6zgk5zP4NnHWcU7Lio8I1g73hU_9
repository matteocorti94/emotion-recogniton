_rotated, error = ImageRotation(faceg)\n","      if (error):\n","          print(\"Points not found\")\n","          openimage += openimage\n","          scarto_point += 1\n","      else:\n","          face_image, error = ImageProcessing(face_rotated)\n","          if (error):\n","            print(\"Face not found, next try\")\n","            face_image, error = NewImageProcessing(face_rotated)  \n","            if (error):            \n","              print(\"Face not found after second attempt\")\n","              openimage += openimage\n","              scarto_face += 1\n","            else:\n","              pil_image = Image.fromarray(face_image)  # Create a PIL imagedraw object\n","              image = FindLandmarks(pil_image, face_image)\n","              if (image == 0):       \n","                  print(\"Landmark not found\")\n","                  openimage += openimage\n","                  scarto_landmark += 1\n","              else:\n","                  debug = debug + 1\n","                  print(debug)\n","                  face_file = image.resize((IMG_WIDTH, IMG_HEIGHT), Image.BILINEAR) #resize the Image\n","                  image_file = \"{}-{}\".format(\"New\",openimage.split(\"/\")[-1])\n","                  finalimage = \"{}{}\".format(new_path, image_file)\n","                  face_file.save(finalimage)                        \n","          else:\n","              pil_image = Image.fromarray(face_image)  # Create a PIL imagedraw object\n","              image = FindLandmarks(pil_image, face_image)\n","              if (image == 0):\n","                print(\"Landmarks not found, next try\")\n","                face_image, error = NewImageProcessing(face_rotated)\n","                if (error):            \n","                  print(\"Face not found after missing landmarks\")\n","                  openimage += openimage\n","                  scarto_face += 1\n","                else:\n","                    pil_image = Image.fromarray(face_image)  # Create a PIL imagedraw object\n","                    image = FindLandmarks(pil_image, face_image)\n","                    if (image == 0):\n","                      print(\"Landmarks not found after second attempt\")\n","                      openimage += openimage\n","                      scarto_landmark += 1\n","                    else:\n","                      debug = debug + 1\n","                      print(debug)\n","                      face_file = image.resize((IMG_WIDTH, IMG_HEIGHT), Image.BILINEAR) #resize the Image\n","                      image_file = \"{}-{}\".format(\"New\",openimage.split(\"/\")[-1])\n","                      finalimage = \"{}{}\".format(new_path, image_file)\n","                      face_file.save(finalimage)        \n","              else:\n","                  debug = debug + 1\n","                  print(debug)\n","                  face_file = image.resize((IMG_WIDTH, IMG_HEIGHT), Image.BILINEAR) #resize the Image\n","                  image_file = \"{}-{}\".format(\"New\",openimage.split(\"/\")[-1])\n","                  finalimage = \"{}{}\".format(new_path, image_file)\n","                  face_file.save(finalimage)\n","  \n","  errori_totali = errori_totali + scarto_point + scarto_face + scarto_landmark\n","  foto_totali = foto_totali + debug\n","\n","  print(\"\\nPunti Not Found: \", scarto_point)\n","  print(\"\\nFace Not Found: \", scarto_face)\n","  print(\"\\nLandmark Not Found: \", scarto_landmark)\n","  print(\"\\nFoto normalizzate: \", debug)\n","  print(\"\\n\\n/////////////////////////////////////////////////////////////////////////////////////////////////////////////\\n\\n\")\n","\n","print(\"\\nErrori Totali: \", errori_totali)\n","print(\"\\nFoto Totali Normalizzate: \", foto_totali)"],"execution_count":8,"outputs":[{"output_type":"stream","text":["/content/gdrive/My Drive/emotion_recognition/data/2_tristezza_def/IMG_5892.JPG\n","1\n","/content/gdrive/My Drive/emotion_recognition/data/2_tri