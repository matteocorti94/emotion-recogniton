code","metadata":{"id":"Bkz0_gRhGH92","colab_type":"code","colab":{}},"source":["def images_in_path(path):\n","    faces = [os.path.join(dirpath, f)\n","        for dirpath, dirnames, files in os.walk(path)\n","            for f in files if f.endswith(\".JPG\")]\n","    return faces\n","\n","\n","\n","#---------------------------------FACE ROTATION-------------------------------------\n","\n","\n","def ImageRotation(face_image):\n","    pts3 = 0\n","    pts2 = 0\n","    pts1 = 0\n","    error = 0\n","\n","    face_points = face_recognition.face_landmarks(face_image, model = \"small\")\n","    if not face_points:\n","        error = 1\n","        return 0, error\n","    for face_landmarks in face_points:\n","        for point in face_landmarks.keys():\n","            pt = face_landmarks[point]\n","            ptx = np.float32(pt)\n","            pts1 = pts2\n","            pts2 = pts3\n","            pts3 = ptx\n","    x_nose = (pts1[0,0])\n","    y_nose = (pts1[0,1])\n","    x_occhio_s = (pts2[0, 0] + pts2[1, 0]) / 2\n","    y_occhio_s = (pts2[0, 1] + pts2[1, 1]) / 2\n","    x_occhio_d = (pts3[0, 0] + pts3[1, 0]) / 2\n","    y_occhio_d = (pts3[0, 1] + pts3[1, 1]) / 2\n","\n","    punti = np.float32([[x_nose, y_nose], [x_occhio_d, y_occhio_d], [x_occhio_s, y_occhio_s]])\n","    punto_fisso = np.float32([[150,185], [75,95], [225,95]]) #parametri presi su più campioni di foto dritte\n","    M = cv.getAffineTransform(punti, punto_fisso)\n","    dst = cv.warpAffine(face_image, M, (700, 700)) #trova bene le misure\n","\n","    return dst, error\n","\n","\n","\n","#---------------------------------FACE RECOGNITION-------------------------------------\n","\n","#method 1\n","def ImageProcessing(face):\n","    error = 0\n","\n","    face_locations = face_recognition.face_locations(face) #find all the faces\n","    #face_locations = face_recognition.face_locations(face, number_of_times_to_upsample=0, model=\"cnn\")\n","    if not face_locations:\n","        error = 1\n","        return 0, error\n","    for face_location in face_locations:  # Print the location of each face in this image top, right, bottom, left = face_location\n","        top, right, bottom, left = face_location\n","        face_image = face[top:bottom, left:right]\n","\n","    return face_image,error\n","\n","#method 2\n","def NewImageProcessing(face):\n","    error = 0\n","\n","    faces = face_cascade.detectMultiScale(faceg, 1.1, 5)\n","    if not (len(faces) > 0):\n","        error = 1\n","        return 0, error\n","\n","    for (x, y, w, h) in faces:\n","        cv.rectangle(faceg, (x, y), (x + w, y + h), (255, 255, 0), 2)\n","        face_image = faceg[y:y + h, x:x + w]\n","\n","\n","    return face_image,error\n","\n","\n","\n","#---------------------------------LANDMARKS-------------------------------------\n","\n","def FindLandmarks (pil_im, face_image):\n","    face_landmarks_list = face_recognition.face_landmarks(face_image) # find landmarks of all the faces\n","    if not face_landmarks_list:\n","        return 0\n","    d = ImageDraw.Draw(pil_im)  # so we can draw on the picture\n","    # Trace out each facial feature in the image with a line!\n","    for face_landmarks in face_landmarks_list:\n","        for facial_feature in face_landmarks.keys():\n","            d.line(face_landmarks[facial_feature], width=1)\n","\n","    return pil_im"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"i2iqk9Qejjie","colab_type":"code","colab":{}},"source":["#rec_path=[\"/content/gdrive/My Drive/emotion_recognition/data/1_gioia_def\",\"/content/gdrive/My Drive/emotion_recognition/data/2_tristezza_def\",\"/content/gdrive/My Drive/emotion_recognition/data/3_paura_def\",\"/content/gdrive/My Drive/emotion_recognition/data/4_rabbia_def\",\"/content/gdrive/My Drive/emotion_recognition/data/5_sorpresa_def\",\"/content/gdrive/My Drive/emotion_recognition/data/6_disgusto_def\",\"/content/gdrive/My Drive/emotion_recognition/data/7_neutrale_def\"]\n","#rec_ne